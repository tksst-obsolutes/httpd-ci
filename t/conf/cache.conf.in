#
# Config for mod_cache tests
#

<IfModule mod_cache.c>
    <IfModule mod_disk_cache.c>

        <VirtualHost mod_cache>
            CacheEnable    disk /cache/
            CacheRoot      @SERVERROOT@/conf/cacheroot/ 
            CacheDirLevels 1
            CacheDirLength 1

            DocumentRoot @SERVERROOT@/htdocs/modules/cache
        </VirtualHost>
   </IfModule>
</IfModule>
